@page
@model RoomSearchPageModel
@{
    ViewData["Title"] = "Search rooms"; 
}

<h1>Look out the available rooms</h1>

<form method="post"> 
    <div class="form-group">
        <label asp-for="StartDate" />
        <input asp-for="StartDate" type="text" placeholder="Type the start date here..." />
        <label asp-for="EndDate" />
        <input asp-for="EndDate" type="text" placeholder="Type the end date here..." /> 

    </div>
    <button type="submit" class="btn btn-primary">Search</button> 
    <div class="form-group">

    </div>
</form>
